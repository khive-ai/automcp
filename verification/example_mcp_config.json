{
  "mcpServers": {
    "data-processor": {
      "command": "uv",
      "args": [
        "run",
        "verification/data_processor_mcp_server.py"
      ],
      "alwaysAllow": [
        "data-processor.process_data",
        "data-processor.generate_report",
        "data-processor.validate_schema"
      ],
      "autoApprove": [
        "data-processor.process_data",
        "data-processor.generate_report",
        "data-processor.validate_schema"
      ]
    }
  }
}
